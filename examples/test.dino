;; testing dino features
    SET %name, 69
    SET %neg, -100
    PRINL %neg
    ABS %neg
    PRINL %neg

    SET %Pi, 3.14
    CEIL %Pi
    PRINT "3.14? I prefer "
    PRINT %Pi
    PRINT ". Or even "
    SET %Pi, 3.14
    FLOOR %Pi
    PRINL %Pi

    PRINL %name
    STORE %age, %name
    PRINT "You're how old? "
    PRINL %age
    SET %thousand 1000
    ADD %thousand, %age, %age
    PRINT "Or are you "
    PRINT %age
    PRINL "?"
    SET %beer, 99
    SET %age, 100
    PRINT "Oh wait... you're "
    PRINT %age
    PRINL " ?!?!"
    PRINT "I thought you were "
    RANDOM %age
    SET %ten, 100
    MUL %age, %ten, %age
    CEIL %age
    PRINL %age

    SET @file, "./examples/test.dino"
    READ @file, @file
    PRINL @file

    EXIT
