;; factorial
    .byte "L", "D", "P", "L", 1
    DISPLAY "Enter a number: "
    ACCEPT %n
    STOREN %result, 1   ; default
while:
    STOREN $y, 0
    GT %n, $y, $a
    JIF end_while 
    MUL %n, %result, %result 
    STOREN $y, 1
    SUB %n, $y, %n
    JUMP while
end_while:
    DISPLAY "Factorial: " 
    DISPLAY %result 
    DISPLAY "\n"