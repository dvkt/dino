;; factorial
    SET %result, 1   ; default
    STORE %n, @arg0
    EQ $ac, 0, $a
    JIF while
    PRINT "ðŸ¦• Enter a number: "
    ACCEPT %n
while:
    SET $y, 0
    GT %n, $y, $a
    JIF end_while
    MUL %n, %result, %result
    SET $y, 1
    SUB %n, $y, %n
    JUMP while
end_while:
    PRINT "Factorial: "
    PRINL %result
    EXIT